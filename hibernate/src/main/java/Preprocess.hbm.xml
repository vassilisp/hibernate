<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
    <class name="gq.panop.hibernate.model.Preprocess" table="Preprocess">
      <id name="transactionId" > <generator class="assigned"/></id>
      <property name="processID"/>
      <property name="clientIP"/>
      <property name="requestedResource" type="string"/>
      <property name="referer"/>
      <property name="userAgent"/>
      <property name="timestamp"/>
      <property name="statusCode"/>
      <property name="clientId"/>
      <property name="userId"/>
      <property name="subSession"/>
      <property name="refererID"/>
      <property name="targetID"/>
   </class>
</hibernate-mapping>